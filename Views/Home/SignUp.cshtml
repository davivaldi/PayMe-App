@model User

 <form asp-action="Register" asp-controller="Home" method="POST">

        <span asp-validation-for="FirstName"></span>
        <label asp-for="FirstName"></label><br>
        <input asp-for="FirstName"> <br><br>

        <span asp-validation-for="LastName"></span>
        <label asp-for="LastName"></label><br>
        <input asp-for="LastName"> <br><br>

        <span asp-validation-for="Email"></span>
        <label asp-for="Email"></label><br>
        <input asp-for="Email"> <br><br>
        

        <span asp-validation-for="Password"></span>
        <label asp-for="Password"></label><br>
        <input asp-for="Password"> <br><br>

        <span asp-validation-for="Confirm"></span>
        <label asp-for="Confirm"></label><br>
        <input asp-for="Confirm"> <br><br>

        <button type="submit" class="btn btn-success">Sign Up</button>
</form>